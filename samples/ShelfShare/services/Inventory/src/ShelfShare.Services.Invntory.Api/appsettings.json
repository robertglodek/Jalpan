{
  "app": {
    "name": "ShelfShare Inventory Service",
    "service": "inventory-service",
    "version": "V1"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "API",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true
  },
  "serilog": {
    "level": "information",
    "overrides": {
      "Microsoft.AspNetCore": "Warning"
    },
    "excludePaths": [
      "/about",
      "/metrics",
      "/ping"
    ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "password",
      "email"
    ],
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": false,
      "url": "http://localhost:5341",
      "apiKey": "secret"
    },
    "mongo": {
      "enabled": false,
      "url": "mongodb://localhost:27017",
      "collection": "Logs"
    },
    "elk": {
      "enabled": false,
      "url": "http://localhost:9200",
      "basicAuthEnabled": false,
      "username": "username",
      "password": "password"
    },
    "tags": {}
  }
}