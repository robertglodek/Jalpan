<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Description>Jalpan.Metrics.OpenTelemetry</Description>
		<Authors>Robert Głodek</Authors>
		<AssemblyName>Jalpan.Metrics.OpenTelemetry</AssemblyName>
		<Title>Jalpan.Metrics.OpenTelemetry</Title>
		<PackageId>Jalpan.Metrics.OpenTelemetry</PackageId>
		<PackageTags>Jalpan.Metrics.OpenTelemetry</PackageTags>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
